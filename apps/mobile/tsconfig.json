{
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./*"
            ]
        },
        "jsx": "react-native",
        "lib": [
            "es2021"
        ],
        "moduleResolution": "bundler",
        "noEmit": true,
        "skipLibCheck": true,
        "target": "esnext"
    },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "node_modules"
    ]
}